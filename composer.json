{
    "name": "sourecode/conventional-commits-bundle",
    "type": "symfony-bundle",
    "description": "Conventional commits integration into symfony",
    "keywords": ["symfony", "convention", "commits", "git", "conventional-commits"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Schilling",
            "email": "jason@sourecode.dev"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "sourecode/conventional-commits": "dev-master",
        "sourecode/git": "dev-master",
        "symfony/config": "^5.3 || ^6.0",
        "symfony/console": "^5.3 || ^6.0",
        "symfony/dependency-injection": "^5.3 || ^6.0",
        "symfony/filesystem": "^5.3 || ^6.0",
        "symfony/finder": "^5.3 || ^6.0",
        "symfony/http-kernel": "^5.3 || ^6.0",
        "symfony/validator": "^5.3 || ^6.0"
    },
    "require-dev": {
        "symfony/framework-bundle": "^5.3 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "SoureCode\\Bundle\\ConventionalCommits\\": "src"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SoureCode\\Bundle\\ConventionalCommits\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
